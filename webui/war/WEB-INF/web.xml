<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>webui.html</welcome-file>
  </welcome-file-list>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>targetServlet</servlet-name>
    <servlet-class>org.apache.ace.server.TargetServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>targetServlet</servlet-name>
    <url-pattern>/webui/targets</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>checkoutServlet</servlet-name>
    <servlet-class>org.apache.ace.server.CheckoutServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>checkoutServlet</servlet-name>
    <url-pattern>/webui/checkout</url-pattern>
  </servlet-mapping>


</web-app>
